{
  "name": "@aureooms/js-cll",
  "description": "Circular Linked Lists for JavaScript",
  "version": "1.0.0",
  "author": "aureooms",
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m",
    "concurrency": 4
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-cll/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-itertools": "4.1.0",
    "@babel/cli": "7.12.16",
    "@babel/core": "7.12.16",
    "@babel/preset-env": "7.12.16",
    "@babel/register": "7.12.13",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.0.1",
    "babel-preset-power-assert": "3.0.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "7.3.0",
    "nyc": "15.1.0",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.7",
    "xo": "0.36.1"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-cll",
  "keywords": [
    "circular",
    "data",
    "doubly",
    "linked",
    "list",
    "structure"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aureooms/js-cll"
  },
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "esdoc": "esdoc",
    "lint": "xo",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "sideEffects": false,
  "xo": {
    "prettier": true,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "camelcase": "off",
      "unicorn/filename-case": "off"
    },
    "ignores": [
      "lib",
      "doc"
    ]
  }
}
